Program to check whether a given number is positive or non-positive:

#include <stdio.h>

int main() {
    float num;
    printf("Enter a number: ");
    scanf("%f", &num);

    if (num > 0) {
        printf("Positive\n");
    } else {
        printf("Non-positive\n");
    }

    return 0;
}
Program to check whether a given number is divisible by 5 or not:

#include <stdio.h>

int main() {
    int num;
    printf("Enter a number: ");
    scanf("%d", &num);

    if (num % 5 == 0) {
        printf("Divisible by 5\n");
    } else {
        printf("Not divisible by 5\n");
    }

    return 0;
}
Program to check whether a given number is even or odd:

#include <stdio.h>

int main() {
    int num;
    printf("Enter a number: ");
    scanf("%d", &num);

    if (num % 2 == 0) {
        printf("Even\n");
    } else {
        printf("Odd\n");
    }

    return 0;
}
Program to check whether a given number is even or odd without using % operator:

#include <stdio.h>

int main() {
    int num;
    printf("Enter a number: ");
    scanf("%d", &num);

    if ((num / 2) * 2 == num) {
        printf("Even\n");
    } else {
        printf("Odd\n");
    }

    return 0;
}
Program to check whether a given number is a three-digit number or not:

#include <stdio.h>

int main() {
    int num;
    printf("Enter a number: ");
    scanf("%d", &num);

    if (num >= 100 && num < 1000) {
        printf("Three-digit number\n");
    } else {
        printf("Not a three-digit number\n");
    }

    return 0;
}
Program to print the greater between two numbers:

#include <stdio.h>

int main() {
    float num1, num2;
    printf("Enter the first number: ");
    scanf("%f", &num1);
    printf("Enter the second number: ");
    scanf("%f", &num2);

    if (num1 > num2) {
        printf("%.2f is greater\n", num1);
    } else if (num2 > num1) {
        printf("%.2f is greater\n", num2);
    } else {
        printf("Both numbers are equal\n");
    }

    return 0;
}
Program to check the roots of a quadratic equation
#include <stdio.h>
#include <math.h>

int main() {
    float a, b, c;
    printf("Enter coefficient a: ");
    scanf("%f", &a);
    printf("Enter coefficient b: ");
    scanf("%f", &b);
    printf("Enter coefficient c: ");
    scanf("%f", &c);

    float discriminant = b * b - 4 * a * c;

    if (discriminant > 0) {
        printf("Real and distinct roots\n");
    } else if (discriminant == 0) {
        printf("Real and equal roots\n");
    } else {
        printf("Imaginary roots\n");
    }

    return 0;
}
Program to check whether a given year is a leap year or not:

#include <stdio.h>

int main() {
    int year;
    printf("Enter a year: ");
    scanf("%d", &year);

    if ((year % 4 == 0 && year % 100 != 0) || (year % 400 == 0)) {
        printf("Leap year\n");
    } else {
        printf("Not a leap year\n");
    }

    return 0;
}
Program to find the greatest among three given numbers:

#include <stdio.h>

int main() {
    float num1, num2, num3;
    printf("Enter the first number: ");
    scanf("%f", &num1);
    printf("Enter the second number: ");
    scanf("%f", &num2);
    printf("Enter the third number: ");
    scanf("%f", &num3);

    if (num1 > num2 && num1 > num3) {
        printf("%.2f is the greatest\n", num1);
    } else if (num2 > num1 && num2 > num3) {
        printf("%.2f is the greatest\n", num2);
    } else if (num3 > num1 && num3 > num2) {
        printf("%.2f is the greatest\n", num3);
    } else {
        printf("Numbers are equal\n");
    }

    return 0;
}
Program to calculate and print profit or loss percentage:

#include <stdio.h>

int main() {
    float cost_price, selling_price;
    printf("Enter the cost price: ");
    scanf("%f", &cost_price);
    printf("Enter the selling price: ");
    scanf("%f", &selling_price);

    float profit_loss = selling_price - cost_price;

    if (profit_loss > 0) {
        float percentage = (profit_loss / cost_price) * 100;
        printf("Profit percentage: %.2f%%\n", percentage);
    } else if (profit_loss < 0) {
        float percentage = (fabs(profit_loss) / cost_price) * 100;
        printf("Loss percentage: %.2f%%\n", percentage);
    } else {
        printf("No profit, no loss\n");
    }

    return 0;
}
Program to check whether a candidate passed or failed:

#include <stdio.h>

int main() {
    int passing_marks = 33;
    int total_marks = 500;  // Assuming each subject is out of 100
    int marks[5];
    int total_obtained_marks = 0;

    for (int i = 0; i < 5; i++) {
        printf("Enter marks for subject %d: ", i + 1);
        scanf("%d", &marks[i]);
        total_obtained_marks += marks[i];
    }

    if (total_obtained_marks >= (passing_marks * 5)) {
        printf("Candidate passed\n");
    } else {
        printf("Candidate failed\n");
    }

    return 0;
}
Program to check whether a given alphabet is in uppercase or lowercase:

#include <stdio.h>
#include <ctype.h>

int main() {
    char character;
    printf("Enter a character: ");
    scanf(" %c", &character);

    if (isupper(character)) {
        printf("Uppercase alphabet\n");
    } else if (islower(character)) {
        printf("Lowercase alphabet\n");
